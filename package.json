    "ioredis": "^4.23.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mint-filter": "^3.0.1",
    "mongoose": "^5.11.19",
    "mysql": "^2.18.1",
    "nestjs-typegoose": "^7.1.38",
    "nodemailer": "^6.5.0",
    "os": "^0.1.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
