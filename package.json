{
  "name": "slmblog-server",
  "version": "0.0.1",
  "description": "slmblog server",
  "author": "ShiLaiMu <qq: 47889187/wechat: shi_lai_mu>",
  "private": false,
  "license": "ISC",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch ",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "reset": "rimraf -rf node_modules dist && yarn",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@dicebear/avatars": "^4.6.9",
    "@dicebear/avatars-female-sprites": "^4.6.9",
    "@dicebear/avatars-human-sprites": "^4.6.9",
    "@dicebear/avatars-male-sprites": "^4.6.9",
    "@hapi/joi": "^17.1.1",
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/common": "^8.0.4",
    "@nestjs/config": "^1.0.0",
    "@nestjs/core": "^8.0.4",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-express": "^8.0.4",
    "@nestjs/schedule": "^1.0.0",
    "@nestjs/swagger": "^5.0.4",
    "@nestjs/throttler": "^2.0.0",
    "@nestjs/typeorm": "^8.0.1",
    "@typegoose/typegoose": "^7.6.3",
    "@types/handlebars": "^4.1.0",
    "@types/nodemailer": "^6.4.4",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dayjs": "^1.10.6",
    "handlebars": "^4.7.7",
    "ioredis": "^4.27.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "mint-filter": "^3.0.1",
    "mysql": "^2.18.1",
    "nestjs-typegoose": "^7.1.38",
    "nodemailer": "^6.6.3",
    "os": "^0.1.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "stacktrace-js": "^2.0.2",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.34"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.2",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.4",
    "@types/express": "^4.17.13",
    "@types/handlebars": "^4.1.0",
    "@types/hapi__joi": "^17.1.7",
    "@types/ioredis": "^4.26.6",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.171",
    "@types/node": "^16.4.0",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "os": "^0.1.2",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}